{
  "compilerOptions": {
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "sourceMap": true,
    "removeComments": false,
    "strict": true,
    "target": "ES2020",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
  },
  "compileOnSave": true,
  "include": ["__generated__", ".eslintrc.js"],
}
